  {
    "id": "ReportFullAccess",
    "label": "Report Full Access",
    "statements": [
      {
        "effect": "Allow",
        "actions": [
          "report:ListReports",
          "report:GetReport",
          "report:CreateReport",
          "report:UpdateReport",
          "report:DeleteReport",
          "report:ExecuteReport",
          "report:ExportReport"
        ],
        "feature": "reports",
        "resource": "em2m:reports:report:*",
        "condition": {
          "StringEquals": {
            "report:ReportType": [
              "vehicle"
            ]
          }
        }
      },
      {
        "effect": "Allow",
        "actions": [
          "report:ListReports",
          "report:GetReport",
          "report:CreateReport",
          "report:UpdateReport",
          "report:DeleteReport",
          "report:ExecuteReport",
          "report:ExportReport"
        ],
        "feature": "maintenance",
        "resource": "em2m:reports:report:*",
        "condition": {
          "ForAnyValue:StringEquals": {
            "report:reportType": [
              ["maintenance"]
            ]
          },
          "ForAnyValue:StringEquals": {
            "ident:orgPath": "${claims:org}"
          }
        }
      }
    ]
  },
